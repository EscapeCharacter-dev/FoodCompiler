namespace Collections;

class Handle = void *;

public record Enumerator
{
	void *Data;        // A pointer to the data.
	size  DataLength;  // The length of the data.
	Enumerator *Next;  // The next enumerator item. null if end
}

public int Main()
{
	int value = 42;
	[mutable_ref] int &ref = &value;
	ref = &value;

	string helloWorld = "Hello, World!";
	const int length = lengthof helloWorld;

	yield = 0;
	return;
}
